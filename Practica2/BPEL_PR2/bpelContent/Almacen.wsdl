<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<wsdl:definitions xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:tns="http://www.example.org/Almacen/" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="Almacen" targetNamespace="http://www.example.org/Almacen/">
  <wsdl:types>
    <xsd:schema targetNamespace="http://www.example.org/Almacen/">
      <xsd:element name="comprobarStock">
        <xsd:complexType>
          <xsd:sequence>
          	<xsd:element name="referenciaProducto" type="xsd:string" />
          	<xsd:element name="numeroUnidades" type="xsd:int"></xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="comprobarStockResponse">
        <xsd:complexType>
          <xsd:sequence>
          	<xsd:element name="mensaje" type="xsd:string" />
          	<xsd:element name="funca" type="xsd:boolean"></xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="actualizarStock">
      	<xsd:complexType>
      		<xsd:sequence>

      			<xsd:element name="referenciaProducto"
      				type="xsd:string">
      			</xsd:element>
      			<xsd:element name="numeroUnidades" type="xsd:int"></xsd:element>
      		</xsd:sequence>
      	</xsd:complexType>
      </xsd:element>
      <xsd:element name="actualizarStockResponse">
      	<xsd:complexType>
      		<xsd:sequence>

      			<xsd:element name="mensaje" type="xsd:string"></xsd:element>
      			<xsd:element name="funca" type="xsd:boolean"></xsd:element>
      		</xsd:sequence>
      	</xsd:complexType>
      </xsd:element>
    </xsd:schema>
  </wsdl:types>
  <wsdl:message name="comprobarStockRequest">
    <wsdl:part element="tns:comprobarStock" name="parameters"/>
  </wsdl:message>
  <wsdl:message name="comprobarStockResponse">
    <wsdl:part element="tns:comprobarStockResponse" name="parameters"/>
  </wsdl:message>
  <wsdl:message name="actualizarStockRequest">
  	<wsdl:part name="parameters" element="tns:actualizarStock"></wsdl:part>
  </wsdl:message>
  <wsdl:message name="actualizarStockResponse">
  	<wsdl:part name="parameters" element="tns:actualizarStockResponse"></wsdl:part>
  </wsdl:message>
  <wsdl:portType name="Almacen">
    <wsdl:operation name="comprobarStock">
      <wsdl:input message="tns:comprobarStockRequest"/>
      <wsdl:output message="tns:comprobarStockResponse"/>
    </wsdl:operation>
    <wsdl:operation name="actualizarStock">
    	<wsdl:input message="tns:actualizarStockRequest"></wsdl:input>
    	<wsdl:output message="tns:actualizarStockResponse"></wsdl:output>
    </wsdl:operation>
  </wsdl:portType>
  <wsdl:binding name="AlmacenSOAP" type="tns:Almacen">

  	<soap:binding style="document"
  		transport="http://schemas.xmlsoap.org/soap/http" />
  	<wsdl:operation name="comprobarStock">

  		<soap:operation
  			soapAction="http://www.example.org/Almacen/comprobarStock" />
  		<wsdl:input>

  			<soap:body use="literal" />
  		</wsdl:input>
  		<wsdl:output>

  			<soap:body use="literal" />
  		</wsdl:output>
  	</wsdl:operation>
  	<wsdl:operation name="actualizarStock">

  		<soap:operation
  			soapAction="http://www.example.org/Almacen/actualizarStock" />
  		<wsdl:input>

  			<soap:body use="literal" />
  		</wsdl:input>
  		<wsdl:output>

  			<soap:body use="literal" />
  		</wsdl:output>
  	</wsdl:operation>
  </wsdl:binding>
  <wsdl:service name="Almacen">
    <wsdl:port binding="tns:AlmacenSOAP" name="AlmacenSOAP">
      <soap:address location="http://localhost:9080/Pr2_Almacen/services/Almacen"/>
    </wsdl:port>
  </wsdl:service>
</wsdl:definitions>
